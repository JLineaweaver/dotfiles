{
  "enabledPlugins": {
    "commit-commands@claude-code-plugins": true,
    "marketplace-auto-update@datadog-claude-plugins": true,
    "dd@datadog-claude-plugins": true,
    "atlassian-remote-mcp@datadog-claude-plugins": true,
    "osx-notifications@datadog-claude-plugins": true,
    "redblue@datadog-claude-plugins": true,
    "go-code-reviewer@datadog-claude-plugins": true,
    "python-code-reviewer@datadog-claude-plugins": true,
    "user-config@datadog-claude-plugins": true,
    "streaming-platform-operators@datadog-claude-plugins": true
  },
  "alwaysThinkingEnabled": true,
  "defaultMode": "plan",
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(cd:*)",
      "Bash(pwd:*)",
      "Bash(cat:*)",
      "Bash(grep:*)",
      "Bash(sed:*)",
      "Bash(awk:*)",
      "Bash(find:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(wc:*)",
      "Bash(sort:*)",
      "Bash(uniq:*)",
      "Bash(echo:*)",
      "Bash(which:*)",
      "Bash(whoami:*)",
      "Bash(date:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git branch:*)",
      "Bash(git show:*)",
      "Bash(env:*)",
      "Bash(printenv:*)",
      "Bash(file:*)",
      "Bash(stat:*)",
      "Bash(du:*)",
      "Bash(df:*)"
    ],
    "ask": [],
    "deny": []
  }
}
